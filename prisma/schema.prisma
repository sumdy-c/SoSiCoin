generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AccountDibila {
    id Int @id @default(autoincrement())
    userName String @unique
    password String
    coins Int
    damage Int
    achievements String[] @default(["hello"])
}

model AllCoins {
    id Int @id @default(autoincrement())
    isCheatCoins Int
    isGLTFCoins Int
    isYanaCoins Int
    isMakarCoins Int
    isSosiskaCoins Int
}